@top Program { expression* }

@skip { space | LineComment }



expression {
  Type |
  String |
  Attr |
  Keyword |
  Application { expression "{" expression* "}" }
  Init { Query "{" expression* "}" }
}

@tokens {
  Query {"query"}

  Type { $[A-Z] $[a-zA-Z_0-9]* }

  String { '"' !["\\]* '"' }

  Attr { $[a-z] $[a-zA-Z_0-9]* } 

  Keyword { "... on"  }  

  LineComment { "#" ![\n]* }

  space { $[ \t\n\r]+ }

  "{" "}"

  
}

@detectDelim